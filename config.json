{
    "output": "output",
    "prettifyHTML": true,
    "port": 3000,
    "cssOptions": {
        "advanced": true,
        "aggressiveMerging": true,
        "compatibility": "*",
        "keepBreaks": false,
        "mediaMerging": true
    },
    "jsOptions": {
        "mangle": false,
        "compress": {
            "sequences": true,
            "properties": true,
            "dead_code": true,
            "drop_debugger": true,
            "unsafe": false,
            "conditionals": true,
            "comparisons": true,
            "evaluate": true,
            "booleans": true,
            "loops": true,
            "unused": true,
            "hoist_funs": true,
            "hoist_vars": false,
            "if_return": true,
            "join_vars": true,
            "cascade": true,
            "side_effects": true,
            "warnings": true
        },
        "output": {
            "indent_start": 0,
            "indent_level": 4,
            "quote_keys": false,
            "space_colon": true,
            "ascii_only": false,
            "inline_script": false,
            "width": 80,
            "max_line_len": 32000,
            "beautify": false,
            "source_map": null,
            "bracketize": false,
            "comments": false,
            "semicolons": true
        }
    }
}